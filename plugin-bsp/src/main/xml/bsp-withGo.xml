<idea-plugin>
    <extensions defaultExtensionNs="com.intellij">
        <programRunner implementation="org.jetbrains.plugins.bsp.golang.debug.BspGoDebugRunner"/>
        <runLineMarkerContributor
                implementationClass="org.jetbrains.plugins.bsp.ui.gutters.BspGoRunLineMarkerContributor"
                language="go"/>
        <additionalLibraryRootsProvider
                implementation="org.jetbrains.plugins.bsp.golang.resolve.BlazeGoAdditionalLibraryRootsProvider"/>
        <treeStructureProvider implementation="org.jetbrains.plugins.bsp.golang.treeview.BlazeGoTreeStructureProvider"
                               order="last"/>
    </extensions>

    <extensions defaultExtensionNs="org.jetbrains.bsp">
        <projectSyncHook implementation="org.jetbrains.plugins.bsp.impl.flow.sync.languages.go.GoProjectSync"/>
        <bspRunHandlerProvider
                implementation="org.jetbrains.plugins.bsp.golang.debug.GoBspRunHandler$GoBspRunHandlerProvider"/>
    </extensions>
    <extensions defaultExtensionNs="com.goide">
        <dlv.positionConverterFactory
                implementation="org.jetbrains.plugins.bsp.golang.debug.BspDlvPosConverterFactory"
                order="first"/>
    </extensions>

    <extensions defaultExtensionNs="com.goide">
        <importResolver
                implementation="org.jetbrains.plugins.bsp.golang.resolve.BlazeGoImportResolver"
                id="Bazel.GoImportResolver"
                order="first"/>
        <packageFactory
                implementation="org.jetbrains.plugins.bsp.golang.resolve.BlazeGoPackageFactory"
                id="Bazel.GoPackageFactory"
                order="first"
        />
    </extensions>
</idea-plugin>
